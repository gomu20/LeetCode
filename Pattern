import java.util.Scanner;
public class Pattern {
	
	public static void main(String[] args) {
		System.out.println("enter input");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		pattern1(num);
	}
	
	
	public static void pattern1(int num){
		for (int i=1;i<=num;i++) {
			for (int j=1;j<=num;j++) {
				System.out.print("*  ");
			}
			System.out.println();
		}
	}

}





