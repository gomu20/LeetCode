import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        System.out.println("enter input");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		pattern1(num);
	}
	
	
	public static void pattern1(int num){
		for (int i=1;i<=num;i++) {
			for (int j=1;j<=i;j++) {
				if((i+j)%2!=0) 
				System.out.print("0 ");
				
				else 
					System.out.print("1 ");
				
			}
			System.out.println();
		}
	}
    }

import java.util.Scanner;
public class Pattern {
	
	public static void main(String[] args) {
		System.out.println("enter input");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		pattern1(num);
	}
	
	
	public static void pattern1(int num){
		int k=1;
		for (int i=1;i<=num;i++) {
			
			for (int j=1;j<=i;j++) {
				System.out.println(k);
				
				
				
			}
			System.out.println();
		}
	}

}

import java.util.Scanner;
public class Pattern {
	
	public static void main(String[] args) {
		System.out.println("enter input");
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		pattern1(num);
	}
	
	
	public static void pattern1(int num){
		
		for (int i=1;i<=num;i++) {
			char k='A';
			for (int j=1;j<=i;j++) {
				System.out.print(k);
				k++;
				
				
			}
			System.out.println();
		}
	}

}
// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class Main {
    public static void main(String[] args) {
         int num=7;
        int spaces=2*num-2;
         for(int i=1;i<=2*num-1;i++){
       
        int stas=i;
        if(i>num)
        
            stas=2*num-i;
            
        

       
            for(int j=1;j<=stas;j++){
                System.out.print("*");
            
        }
        for(int j=1;j<=spaces;j++){
            
                System.out.print(" ");
            
        }
        for(int j=1;j<=stas;j++){
                System.out.print("*");
            
        }
        
       
        if(i<num)spaces-=2;
        else spaces +=2;
        
        System.out.println();
        }

}
    
}
// Online Java Compiler
