class Main {
    public static void main(String[] args) {
        int num=4;
         for(int i=1;i<=num;i++){
             for(int j=1;j<=num;j++){
                if((i==1)||(i==num)||(j==1)||(j==num)){
             System.out.print("* ");
                }
                else{
                     System.out.print(" ");
                }
                
         }
         System.out.println();
        
        }

}
    
}
// Online Java Compiler
// Use this editor to write, compile and run your Java code online

class pt {
    public static void main(String[] args) {
        int num=4;
         for(int i=0;i<num;i++){
             for(int j=0;j<num;j++){
                if((i==0)||(j==0)||(i==num-1)||(j==num-1)){
             System.out.print("* ");
                }
                else{
                     System.out.print(" " +" ");
                }
                
         }
         System.out.println();
        
        }

}
    
}
